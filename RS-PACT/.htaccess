Options +FollowSymLinks
RewriteEngine on
RewriteBase /

Options -Indexes

#RewriteRule index.html$ index.php
#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule ^/admin/(.*)$ admin/index.php?p=$1 [L,QSA]
#RewriteRule admin/^([^/]*).htm$ pages.php?id=$1

#Server
ErrorDocument 404 https://pact.riversand.com/404.html
ErrorDocument 403 https://pact.riversand.com/404.html
